{
  "name": "tree-sitter",
  "version": "0.19.5",
  "description": "Incremental parsers for nodejs",
  "author": "Alex313031",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alex313031/node-tree-sitter.git"
  },
  "keywords": [
    "parser",
    "lexer"
  ],
  "main": "index.js",
  "types": "tree-sitter.d.ts",
  "dependencies": {
    "nan": "^2.17.0",
    "prebuild-install": "^6.1.4"
  },
  "devDependencies": {
    "@types/node": "^14.18.44",
    "chai": "^4.3.7",
    "mocha": "^8.4.0",
    "prebuild": "^10.0.1",
    "rimraf": "^3.0.2",
    "superstring": "git+https://github.com/Alex313031/superstring-ng.git#3383235",
    "tree-sitter-javascript": "git+https://github.com/tree-sitter/tree-sitter-javascript.git#master"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "build": "node-gyp build",
    "distclean": "rimraf build && rimraf prebuilds",
    "clean": "rimraf build && rimraf prebuilds && rimraf node_modules",
    "prebuild": "prebuild -r electron -t 12.2.3 --strip && prebuild -t 12.18.3 -t 14.21.3 --strip",
    "prebuild:upload": "prebuild --upload-all",
    "test": "mocha"
  }
}
